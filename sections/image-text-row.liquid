{%- liquid
  assign lazy_load = 'lazy'
  assign fetch_priority = 'auto'

  if section.index == 1
    assign lazy_load = 'auto'
    assign fetch_priority = 'high'
  endif

  if section.settings.aspect_ratio == 'adapt'
    assign aspect_padding_bottom = 1 | divided_by: section.settings.image.aspect_ratio | times: 100 | append: '%'
  else
    assign aspect_padding_bottom = section.settings.aspect_ratio
    assign mobile_aspect_padding_bottom = section.settings.mobile_aspect_ratio
  endif

  if section.settings.mobile_aspect_ratio == 'adapt'
    assign mobile_aspect_padding_bottom = 1 | divided_by: section.settings.image.aspect_ratio | times: 100 | append: '%'
  else
    assign mobile_aspect_padding_bottom = section.settings.mobile_aspect_ratio
  endif

  assign legacy_eyebrow = ''
  assign legacy_title = ''
  assign legacy_body_primary = ''
  assign legacy_body_primary_plain = ''
  assign legacy_stat_count = 0
  assign legacy_name_count = 0
  assign legacy_button_label = section.settings.button_label | strip

  for block in section.blocks
    case block.type
      when 'eyebrow'
        if legacy_eyebrow == blank and block.settings.eyebrow_text != blank
          assign legacy_eyebrow = block.settings.eyebrow_text
        endif
      when 'title'
        if legacy_title == blank and block.settings.title != blank
          assign legacy_title = block.settings.title
        endif
      when 'text'
        if block.settings.text != blank
          if legacy_body_primary == blank
            assign legacy_body_primary = block.settings.text
          endif
        endif
      when 'richtext'
        if block.settings.text != blank
          if legacy_body_primary == blank
            assign legacy_body_primary = block.settings.text
          endif
        endif
      when 'legacy_stat'
        assign legacy_stat_count = legacy_stat_count | plus: 1
      when 'legacy_name'
        assign legacy_name_count = legacy_name_count | plus: 1
    endcase
  endfor

  if legacy_eyebrow == blank
    assign legacy_eyebrow = 'Made in Germany'
  endif
  if legacy_title == blank
    assign legacy_title = '70 years of podiatry.|Now at <em>your door.</em>'
  endif
  if legacy_body_primary == blank
    assign legacy_body_primary = 'Every Stride orthotic is designed in Oberkochen, Germany by <strong>Dietmar Walter</strong> and his third-generation clinic - the same team top hospitals and physical therapists around the world trust to prescribe orthotics to their own patients.'
  endif
  assign legacy_body_primary_plain = legacy_body_primary | strip_html | strip
  if legacy_body_primary_plain == blank or legacy_body_primary_plain contains 'With over 200,000 orthotics made over 70 years'
    assign legacy_body_primary_plain = 'Every Stride orthotic is designed in Oberkochen, Germany by Dietmar Walter and his third-generation clinic — the same team top hospitals and physical therapists around the world trust to prescribe orthotics to their own patients.'
  endif
  if legacy_button_label == blank or legacy_button_label == 'Learn more' or legacy_button_label == 'Learn More'
    assign legacy_button_label = 'Learn the full story &rarr;'
  endif
-%}

{%- style -%}
  #shopify-section-{{section.id }} {
    --text-color: rgb({{section.settings.color_scheme.settings.text.red }}, {{section.settings.color_scheme.settings.text.green }}, {{section.settings.color_scheme.settings.text.blue }});
  }

  .section-{{ section.id }} {
    padding-top: {{ section.settings.padding_top }}px;
    padding-bottom: {{ section.settings.padding_bottom }}px;
  }

  #shopify-section-{{ section.id }} .section__main {
    display: grid;
    grid-template-columns: 1fr;
    gap: 48px;
    align-items: start;
  }

  #shopify-section-{{ section.id }} .legacy-photo-wrap {
    position: relative;
  }

  #shopify-section-{{ section.id }} .legacy-photo-wrap::before {
    content: '';
    position: absolute;
    top: -16px;
    left: 0;
    width: 80px;
    height: 4px;
    border-radius: 2px;
    background: #eacba4;
  }

  #shopify-section-{{ section.id }} .legacy-photo {
    width: 100%;
    aspect-ratio: 4 / 3;
    object-fit: cover;
    object-position: center top;
    border-radius: 12px;
    display: block;
  }

  #shopify-section-{{ section.id }} .image-text-row__image-caption {
    margin-top: 12px;
    font-size: 12px;
    color: #6b7280;
    line-height: 1.45;
  }

  #shopify-section-{{ section.id }} .image-text-row__image-caption p {
    margin: 0;
  }

  #shopify-section-{{ section.id }} .legacy-names {
    display: grid;
    grid-template-columns: repeat(2, minmax(0, 1fr));
    gap: 24px;
    margin-top: 14px;
  }

  #shopify-section-{{ section.id }} .legacy-name-tag {
    border-left: 2px solid #eacba4;
    padding-left: 10px;
  }

  #shopify-section-{{ section.id }} .legacy-name-label {
    font-size: 13px;
    line-height: 1.3;
    font-weight: 700;
    color: #111827;
  }

  #shopify-section-{{ section.id }} .legacy-name-sub {
    margin-top: 2px;
    font-size: 11px;
    line-height: 1.4;
    color: #6b7280;
  }

  #shopify-section-{{ section.id }} .legacy-text {
    display: flex;
    flex-direction: column;
    gap: 24px;
  }

  #shopify-section-{{ section.id }} .legacy-eyebrow {
    display: inline-flex;
    align-items: center;
    gap: 10px;
    font-size: 12px;
    font-weight: 700;
    letter-spacing: 0.2em;
    text-transform: uppercase;
    color: #e0b989;
  }

  #shopify-section-{{ section.id }} .legacy-eyebrow::before {
    content: '';
    width: 24px;
    height: 2px;
    border-radius: 1px;
    background: #e0b989;
    flex-shrink: 0;
  }

  #shopify-section-{{ section.id }} .legacy-heading {
    margin: 0;
    font-size: 56px;
    font-weight: 700;
    line-height: 1.15;
    letter-spacing: -0.01em;
    color: #111827;
  }

  #shopify-section-{{ section.id }} .legacy-heading em,
  #shopify-section-{{ section.id }} .legacy-heading span {
    font-style: normal;
    color: #eacba4;
  }

  #shopify-section-{{ section.id }} .legacy-body {
    margin: 0;
    max-width: 480px;
    font-size: 18px;
    font-weight: 400;
    line-height: 1.5;
    color: #1f2937;
  }

  #shopify-section-{{ section.id }} .legacy-body strong {
    color: #111827;
    font-weight: 700;
  }

  #shopify-section-{{ section.id }} .legacy-body--muted {
    color: #8d94a1;
  }

  #shopify-section-{{ section.id }} .legacy-stats {
    display: grid;
    grid-template-columns: repeat(3, minmax(0, 1fr));
    gap: 20px;
    padding-top: 12px;
    border-top: 1px solid rgba(201, 169, 110, 0.25);
  }

  #shopify-section-{{ section.id }} .legacy-stat-number {
    font-size: 24px;
    line-height: 1.05;
    font-weight: 700;
    color: #111827;
  }

  #shopify-section-{{ section.id }} .legacy-stat-label {
    margin-top: 4px;
    font-size: 12px;
    line-height: 1.4;
    color: #6b7280;
  }

  #shopify-section-{{ section.id }} .legacy-link {
    display: inline-flex;
    width: fit-content;
    font-size: 15px;
    font-weight: 600;
    color: #111827;
    text-decoration: none;
    border-bottom: 1.5px solid #eacba4;
    padding-bottom: 2px;
    margin-top: 2px;
  }

  @media screen and (min-width: 768px) {
    #shopify-section-{{ section.id }} .section__main { grid-template-columns: 1fr 1fr; gap: 80px; }
  }

  @media screen and (max-width: 900px) {
    .section-{{ section.id }} {
      padding-top: {{ section.settings.padding_top | times: 0.6 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.6 | round: 0 }}px;
    }

    #shopify-section-{{ section.id }} .legacy-heading { font-size: 32px; }
    #shopify-section-{{ section.id }} .legacy-body { font-size: 16px; line-height: 1.45; }
    #shopify-section-{{ section.id }} .legacy-stats { grid-template-columns: 1fr; gap: 14px; }
    #shopify-section-{{ section.id }} .legacy-names { grid-template-columns: 1fr; gap: 10px; }
  }
{%- endstyle -%}

<div class="section-{{ section.id }} gradient color-{{ section.settings.color_scheme }}">
  <div class="section__main page-width {%  if section.settings.container_size == 'narrow' %}md:max-w-screen-lg{% endif %}">
    <div class="legacy-photo-wrap">
      {%- if section.settings.image != blank -%}
        {{
          section.settings.image
          | image_url: width: 1800
          | image_tag: class: 'legacy-photo', loading: lazy_load, fetchpriority: fetch_priority, alt: section.settings.image.alt
        }}
      {%- else -%}
        {{ 'legacy-team.jpg' | asset_url | image_tag: class: 'legacy-photo', alt: 'Dietmar Walter and team in Oberkochen clinic' }}
      {%- endif -%}

      <div class="legacy-names">
        {%- if legacy_name_count > 0 -%}
          {%- for block in section.blocks -%}
            {%- if block.type == 'legacy_name' -%}
              <div class="legacy-name-tag" {{ block.shopify_attributes }}>
                <div class="legacy-name-label">{{ block.settings.name_label }}</div>
                <div class="legacy-name-sub">{{ block.settings.name_sub }}</div>
              </div>
            {%- endif -%}
          {%- endfor -%}
        {%- else -%}
          <div class="legacy-name-tag">
            <div class="legacy-name-label">Dietmar Walter</div>
            <div class="legacy-name-sub">Orthopaedic Innovator &amp; Seifriz Award Winner</div>
          </div>
          <div class="legacy-name-tag">
            <div class="legacy-name-label">Dominik Walter</div>
            <div class="legacy-name-sub">Clinical Operations, Oberkochen</div>
          </div>
        {%- endif -%}
      </div>
    </div>

    <div class="legacy-text">
      <div class="legacy-eyebrow">{{ legacy_eyebrow }}</div>

      <h2 class="legacy-heading">
        {{
          legacy_title
          | replace: '<em>', '<span>'
          | replace: '</em>', '</span>'
          | replace: '|', '<br>'
        }}
      </h2>

      <p class="legacy-body">{{ legacy_body_primary_plain | replace: 'Dietmar Walter', '<strong>Dietmar Walter</strong>' }}</p>

      <div class="legacy-stats">
        {%- if legacy_stat_count > 0 -%}
          {%- for block in section.blocks -%}
            {%- if block.type == 'legacy_stat' -%}
              <div class="legacy-stat" {{ block.shopify_attributes }}>
                <div class="legacy-stat-number">{{ block.settings.stat_number }}</div>
                <div class="legacy-stat-label">{{ block.settings.stat_label | newline_to_br }}</div>
              </div>
            {%- endif -%}
          {%- endfor -%}
        {%- else -%}
          <div class="legacy-stat">
            <div class="legacy-stat-number">Est. 1995</div>
            <div class="legacy-stat-label">Dr. Walter's clinic,<br>Oberkochen Germany</div>
          </div>
          <div class="legacy-stat">
            <div class="legacy-stat-number">60+</div>
            <div class="legacy-stat-label">Countries using<br>Dr. Walter's systems</div>
          </div>
          <div class="legacy-stat">
            <div class="legacy-stat-number">Seifriz Award</div>
            <div class="legacy-stat-label">Adalbert Seifriz Award<br>for innovation, 2000</div>
          </div>
        {%- endif -%}
      </div>

      <a href="{{ section.settings.button_url | default: '#' }}" class="legacy-link">{{ legacy_button_label }}</a>
    </div>
  </div>
</div>

