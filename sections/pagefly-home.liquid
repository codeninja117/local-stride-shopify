<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Testimonial Carousel</title>
  <style>
    .carousel-container {
      width: 100%;
      overflow: hidden;
      position: relative;
      background-color: #fff;
      border-radius: 8px;
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
      padding: 20px;
      font-family: manrope !important;
    }

    .slides {
      display: flex;
      transition: transform 0.5s ease-in-out;
      font-family: manrope !important;
    }

    .slide {
      min-width: 100%;
      display: grid;
      grid-template-columns: 1fr;
      /* Default to 1 column */
      gap: 20px;
      padding: 20px;
      box-sizing: border-box;
    }

    .testimonial {
      text-align: start;
      padding: 10px;

      border-radius: 8px;

    }

    .testimonial p {

      font-size: 28px;
      color: #000000;
      margin-bottom: 10px;
      /* Reduced margin */
      line-height: 30px !important;
    }

    .testimonial .author {
      display: flex;
      align-items: center;
      justify-content: start;
      gap: 12px;
    }

    .testimonial img {
      width: 70px;
      height: 70px;
      border-radius: 50%;
      margin-top: 20px !important;
    }

    .testimonial .author-details {
      text-align: left;

    }

    .testimonial .author-details h4 {
      font-size: 22px;
      font-weight: bold;
      color: #333;
    }

    .testimonial .author-details p {
      font-size: 16px;
      color: #777;

      margin-top: -20px !important;
    }

    .controls {
      display: flex;
      justify-content: end;
      align-items: center;
      margin-top: 120px;
      gap: 10px;
    }

    .controls button {
      background-color: #e0e0e0;
      border: none;
      padding: 15px 30px;
      border-radius: 20px;
      cursor: pointer;
      transition: background-color 0.3s;
      font-size: 16px !important;
      font-weight: bold;
    }

    .controls button:hover {
      background-color: #ccc;
    }

    .index {
      font-size: 16px;
      font-weight: bold;
      color: #333;
    }

    /* Mobile screen adjustments */
    @media (max-width: 768px) {
      .slide {
        grid-template-columns: 1fr;
        /* Change to single column */
      }

      .testimonial p {
        font-size: 16px;
        line-height: 20px;
      }

      .testimonial .author-details h4 {
        font-size: 16px !important;
      }

      .testimonial .author-details p {
        font-size: 14px !important;
