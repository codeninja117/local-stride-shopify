    <!-- Script to capture MSCLKID from URL and store it -->
    <script>
      (function () {
        // Helper to get a URL parameter by name
        function getParam(name) {
          const params = new URLSearchParams(window.location.search);
          return params.get(name);
        }
 
        const msclkid = getParam('msclkid');
        if (msclkid) {
          // Save MSCLKID in a cookie (90-day expiration)
          const expiry = new Date(Date.now() + 90 * 24 * 60 * 60 * 1000).toUTCString();
          document.cookie = `msclkid=${msclkid}; expires=${expiry}; path=/; SameSite=Lax`;

          // Also save in localStorage as a backup
          try {
            localStorage.setItem('msclkid', msclkid);
          } catch (e) {
            console.warn('Could not write msclkid to localStorage', e);
          }
        }
      })();
