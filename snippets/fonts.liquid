{% style %}
  @font-face {
    font-family: 'Manrope';
    src: url({{'Manrope-Light.woff2'|asset_url}}) format('woff2'),
         url({{'Manrope-Light.woff'|asset_url}}) format('woff');
    font-weight: 300;
    font-style: normal;
    font-display: swap;
  }
  @font-face {
    font-family: 'Manrope';
    src: url({{'Manrope-Regular.woff2'|asset_url}}) format('woff2'),
         url({{'Manrope-Regular.woff'|asset_url}}) format('woff');
    font-weight: 400;
    font-style: normal;
    font-display: swap;
  }

  @font-face {
    font-family: 'Manrope';
    src: url({{'Manrope-Medium.woff2'|asset_url}}) format('woff2'),
         url({{'Manrope-Medium.woff'|asset_url}}) format('woff');
    font-weight: 500;
    font-style: normal;
    font-display: swap;
  }

  @font-face {
    font-family: 'Manrope';
    src: url({{'Manrope-SemiBold.woff2'|asset_url}}) format('woff2'),
         url({{'Manrope-SemiBold.woff'|asset_url}}) format('woff');
    font-weight: 600;
    font-style: normal;
    font-display: swap;
  }

  @font-face {
    font-family: 'Manrope';
    src: url({{'Manrope-Bold.woff2'|asset_url}}) format('woff2'),
         url({{'Manrope-Bold.woff'|asset_url}}) format('woff');
    font-weight: 700;
    font-style: normal;
    font-display: swap;
  }
{% endstyle %}
